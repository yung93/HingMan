{
  "name": "hingman",
  "version": "1.0.0",
  "description": "Hing Man data server",
  "main": "index.js",
  "scripts": {
    "build": "nodemon --exec babel-node src -d dist",
    "start": "npm run build && nodemon dist/index.js",
    "prod": "node dist/index.js",
    "migrate": "sequelize db:migrate",
    "migrate:down": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "clear": "npm run migrate:down && npm run migrate && npm run seed"
  },
  "author": "yung",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.1"
  }
}
